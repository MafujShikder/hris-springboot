type RelationalFilter{
    id: ID!
    fieldId:String
    uid:String
    name: String
    excludefieldoptions: String
    datecreated: String
    lastupdated: String
    field:field
}
extend type Query {
    relationalFilters:[RelationalFilter]!
    getRelationalFilterById(id:ID!):RelationalFilter
    queryRelationalFilters(where:String!):[RelationalFilter]!
}

extend type Mutation {
    newRelationalFilter(fieldId:String!, uid:String!, name: String!, excludefieldoptions: String!, field:ID!):RelationalFilter!

    deleteRelationalFilter(id:ID!):String

    updateRelationalFilter(id:ID!, fieldId:String, uid:String, name: String ,excludefieldoptions: String, field:ID):RelationalFilter!
}