type FieldOptionGroup{
    id:ID
    fieldId:Int
    uid:String
    operator:String
    name:String
    description:String
    datecreated:String
    lastupdated:String
    field:field
}

extend type Query {
    fieldOptionGroups:[FieldOptionGroup]!
    getFieldOptionGroupById(id:ID!):FieldOptionGroup
    queryFieldOptionGroup(where:String!):[FieldOptionGroup]!
}

extend type Mutation {
    newFieldOptionGroup(uid:String!, name:String!, description:String!, fieldId:String!,operator:String!,field:ID):FieldOptionGroup!

    deleteFieldOptionGroup(id:ID!):String

    updateFieldOptionGroup(id:ID!,uid:String, name:String, description:String,fieldId:String,operator:String,field:ID):FieldOptionGroup

}